  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.100",
    "@swc/jest": "0.2.29",
    "@types/jest": "29.5.10",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "semantic-release": "^22.0.8",
    "ts-node": "10.9.1",
